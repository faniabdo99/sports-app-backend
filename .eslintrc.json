{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": ["error", "always"],
        "no-param-reassign": "off",
        "consistent-return": "off",
        "import/no-named-as-default-member": "off",
        "import/no-named-as-default": "off"
    }
}
